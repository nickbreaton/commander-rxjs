{
  "name": "commander-rxjs",
  "version": "0.0.4",
  "description": "A simple adapter to easily use RxJS with commander.",
  "main": "dist/index.js",
  "repository": "https://github.com/nickbreaton/commander-rxjs.git",
  "author": "nickbreaton",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist --ignore test.js",
    "coverage": "jest --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "example:calculator": "babel-node examples/calculator",
    "example:json-fetch": "babel-node examples/json-fetch",
    "prepublish": "npm run build",
    "test": "jest --verbose"
  },
  "dependencies": {
    "camel-case": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "commander": "^2.9.0",
    "jest-cli": "^17.0.3",
    "node-fetch": "^1.6.3",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0-rc.4",
    "serve": "^1.4.0"
  },
  "peerDependencies": {
    "commander": "2.x",
    "rxjs": "~5.0.0 || ^5.0.0-rc.1"
  }
}
